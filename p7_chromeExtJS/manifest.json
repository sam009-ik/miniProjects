{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Leads tracker",
    "description": "Saves tabs and track leads",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "*://*/*"
    ],

    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    }
}
